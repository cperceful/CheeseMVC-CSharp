@using CheeseMVC.Models;

<h1>Remove Cheese</h1>

<form method="post">
    <select name="removeCheese">
        @foreach (Cheese cheese in ViewBag.cheeses)
        {
            <option value="@cheese.Name">@cheese.Name</option>

        }
    </select>
    <input type="submit" value="Remove this cheese from my sight"/>

</form>  
