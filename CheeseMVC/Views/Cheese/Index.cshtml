@{
    Layout = null;


 }

<h1>My Cheese</h1>

<p><a asp-controller="Cheese" asp-action="Add">Add Cheese</a></p>

@if (ViewBag.cheeses.Count == 0)
{
    <p>You have no cheese.</p>

}

<ul>
    @foreach (KeyValuePair<string, string> cheese in ViewBag.cheeses) //variable must be called by ViewBag.variable
    {
    <li>@cheese.Key, @cheese.Value</li>
    }
</ul>